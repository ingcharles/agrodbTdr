	<subDataset name="New Dataset 1" uuid="ef677ce2-d5e5-48de-b27c-92231fea0db8">
		<parameter name="idPagoNotaCredito" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
     detalle_nota_credito."concepto_nota_credito" AS concepto_nota_credito,
     detalle_nota_credito."cantidad" AS cantidad,
     detalle_nota_credito."precio_unitario" AS precio_unitario,
     detalle_nota_credito."descuento" AS detalle_pago_descuento,
     detalle_nota_credito."iva" AS detalle_pago_iva,
     detalle_nota_credito."total" AS detalle_pago_total,
     servicios."codigo" AS servicios_codigo
FROM
     "g_financiero"."servicios" servicios INNER JOIN "g_financiero"."detalle_nota_credito" detalle_nota_credito ON servicios."id_servicio" = detalle_nota_credito."id_servicio"
WHERE
     id_nota_credito = $P{idPagoNotaCredito}]]>
		</queryString>
		<field name="concepto_nota_credito" class="java.lang.String"/>
		<field name="cantidad" class="java.lang.Double"/>
		<field name="precio_unitario" class="java.math.BigDecimal"/>
		<field name="detalle_pago_descuento" class="java.math.BigDecimal"/>
		<field name="detalle_pago_iva" class="java.math.BigDecimal"/>
		<field name="detalle_pago_total" class="java.math.BigDecimal"/>
		<field name="servicios_codigo" class="java.lang.String"/>
	</subDataset>
	<parameter name="idnotaCredito" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ivaSri" class="java.lang.String"/>
	<parameter name="fondoCertificado" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     nota_credito."identificador_operador" AS nota_credito_identificador_operador,
     oficina_recaudacion."numero_establecimiento" ||'-'|| nota_credito."punto_emision" ||'-'||nota_credito."numero_nota_credito" AS nota_credito_numero_nota_credito,
     nota_credito."fecha_nota_credito" AS nota_credito_fecha_nota_credito,
     nota_credito."total_pagar" AS nota_credito_total_pagar,
     nota_credito."motivo" AS nota_credito_motivo,
     nota_credito."localizacion" AS nota_credito_localizacion,
     nota_credito."numero_autorizacion" AS orden_pago_numero_autorizacion,
     nota_credito."fecha_autorizacion" AS orden_pago_fecha_autorizacion,
     nota_credito."clave_acceso" AS orden_pago_clave_acceso,
     nota_credito."tipo_emision" AS nota_credito_tipo_emision,
     distritos."ruc" AS distritos_ruc,
     distritos."razon_social" AS distritos_razon_social,
     distritos."direccion" AS distritos_direccion,
     distritos."obligado_llevar_contabilidad" AS distritos_obligado_llevar_contabilidad,
     clientes."razon_social" AS clientes_razon_social,
     clientes."direccion" AS clientes_direccion,
     clientes."telefono" AS clientes_telefono,
     clientes."correo" AS clientes_correo,
     clientes."identificador" AS clientes_identificador,
     orden_pago."numero_establecimiento" ||'-'|| orden_pago."punto_emision" ||'-'||orden_pago."numero_factura" AS orden_pago_numero_factura,
     orden_pago."fecha_facturacion" AS orden_pago_fecha_facturacion,
     (select sum(total)::numeric(8,2) from g_financiero.detalle_nota_credito where id_nota_credito  = $P{idnotaCredito} and iva = 0) as total_sin_iva,
     (select sum(total)::numeric(8,2) - sum(iva)::numeric(8,2) from g_financiero.detalle_nota_credito where id_nota_credito  = $P{idnotaCredito} and iva != 0) as total_con_iva,
     (select sum(descuento)::numeric(8,2) from g_financiero.detalle_nota_credito where id_nota_credito  = $P{idnotaCredito}) as descuento,
     (select sum(iva)::numeric(8,2) from g_financiero.detalle_nota_credito where id_nota_credito  = $P{idnotaCredito}) as suma_iva
FROM
     "g_financiero"."nota_credito" nota_credito INNER JOIN "g_financiero"."clientes" clientes ON nota_credito."identificador_operador" = clientes."identificador"
     INNER JOIN "g_financiero"."distritos" distritos ON nota_credito."ruc_institucion" = distritos."ruc"
     INNER JOIN "g_financiero"."orden_pago" orden_pago ON orden_pago."id_pago" = nota_credito."id_pago"
     INNER JOIN "g_financiero"."oficina_recaudacion" oficina_recaudacion ON distritos."ruc" = oficina_recaudacion."ruc"
WHERE
     nota_credito."id_nota_credito" = $P{idnotaCredito} and oficina_recaudacion."identificador_firmante" = (SELECT nota_credito."identificador_usuario" FROM "g_financiero"."nota_credito" nota_credito WHERE nota_credito."id_nota_credito" = $P{idnotaCredito} )]]>
	</queryString>
