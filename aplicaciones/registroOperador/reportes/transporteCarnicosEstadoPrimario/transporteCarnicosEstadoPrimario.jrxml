		<queryString language="SQL">
			<![CDATA[SELECT 
					pr.id_producto, 
					nombre_comun, 
					sp.nombre as nombre_subtipo, 
					tp.nombre as nombre_tipo, 
					o.id_operacion 
				FROM 
				g_operadores.operaciones o, 
				g_catalogos.productos pr, 
				g_catalogos.subtipo_productos sp ,
				g_catalogos.tipo_productos tp
				WHERE 
				 o.id_producto = pr.id_producto and
				 pr.id_subtipo_producto = sp.id_subtipo_producto and
				 sp.id_tipo_producto = tp.id_tipo_producto and 
				 o.id_operador_tipo_operacion = 	 $P{idOperadorTipoOperacion} 		
				 and o.id_operacion not in (SELECT id_operacion FROM g_operadores.operaciones_organico oo WHERE oo.id_operacion = o.id_operacion)]]>
