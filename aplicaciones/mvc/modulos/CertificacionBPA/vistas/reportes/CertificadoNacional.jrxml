	<subDataset name="grupalEquivalente" uuid="0fcfbb83-8bd4-4224-acd3-0650f7bd87bc">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="local"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w1" value="455"/>
		<property name="com.jaspersoft.studio.property.dataset.dialog.DatasetDialog.sash.w2" value="531"/>
		<parameter name="id_solicitud" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[$P{id_solicitud}]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
	count(distinct o.identificador) as numero_socios
	, string_agg(distinct sap.nombre_producto, ', ') as productos
	, sum(sap.superficie) as hectareas_totales
FROM
	g_certificacion_bpa.sitios_areas_productos sap
	INNER JOIN g_operadores.sitios s ON sap.id_sitio = s.id_sitio
	INNER JOIN g_operadores.operadores o ON s.identificador_operador = o.identificador
WHERE
	sap.id_solicitud =  $P{id_solicitud}]]>
		</queryString>
		<field name="numero_socios" class="java.lang.Long">
			<property name="com.jaspersoft.studio.field.label" value="numero_socios"/>
		</field>
		<field name="productos" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="productos"/>
		</field>
		<field name="hectareas_totales" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.label" value="hectareas_totales"/>
		</field>
	</subDataset>
	<parameter name="idSolicitud" class="java.lang.Integer"/>
	<parameter name="identificador" class="java.lang.String"/>
	<parameter name="rutaFondo" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT
	s.*,
	gbp.nombre_resolucion,
	gbp.numero_resolucion,
	gbp.fecha_resolucion,
	CASE WHEN s.es_asociacion = 'Si' AND s.tipo_solicitud = 'Equivalente'
	THEN
		'Si'
	ELSE
		'No'
	END as validar_numero_miembros,
	CASE WHEN s.es_asociacion = 'Si' AND s.tipo_solicitud = 'Equivalente'
	THEN
		'Detalle del Grupo de Productores:'
	ELSE
		'Sitios de producción:'
	END as titulo_tabla,
	(
		SELECT 
			distinct RTRIM(array_to_string(array_agg('P)' || si.provincia || '-' ||sap.nombre_producto), ', '),'') as detalle_productos
		FROM
			g_certificacion_bpa.sitios_areas_productos sap 
			INNER JOIN g_operadores.sitios si ON sap.id_sitio = si.id_sitio
		WHERE 
			sap.id_solicitud = s.id_solicitud
		ORDER BY
			1
	),
	(
	SELECT
						cargo
  					FROM
						g_catalogos.responsables_certificados
					WHERE 
						identificador =  '$P!{identificador}'
						and estado = 'Activo'
	) as area_director,
	(
	SELECT
						nombre
  					FROM
						g_catalogos.responsables_certificados
					WHERE 
						identificador =  '$P!{identificador}'
						and estado = 'Activo'
	) as nombre_director
FROM
	g_certificacion_bpa.solicitudes s
	FULL OUTER JOIN g_catalogos.guias_buenas_practicas gbp ON s.id_resolucion = gbp.id_guia_buenas_practicas
WHERE
	s.id_solicitud =   $P{idSolicitud}]]>
	</queryString>
