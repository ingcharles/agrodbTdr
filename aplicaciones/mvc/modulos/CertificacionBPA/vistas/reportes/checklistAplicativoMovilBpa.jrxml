	<parameter name="idSolicitud" class="java.lang.Integer"/>
	<parameter name="rutaLogoAgro" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
                        		--s.id_solicitud
                        		 s.identificador

                        		, s.es_asociacion
                        		, s.tipo_solicitud
                        		, s.tipo_explotacion
                        		, s.identificador_operador
                        		, UPPER(CASE WHEN o.razon_social = '' THEN o.nombre_representante ||' '|| o.apellido_representante ELSE o.razon_social END) AS nombre_operador
                        		, st.nombre_lugar AS sitio_unidad_produccion
                        		,  STRING_AGG(sap.nombre_producto, ', ') AS nombre_productos
                        		--, CASE WHEN s.estado = 'registrado' THEN 'Aprobado' ELSE s.estado END  AS estado
                        		, to_char(s.fecha_auditoria_programada, 'YYYY-MM-DD') AS fecha_auditoria_programada
                        		, s.provincia_revision
                        		, to_char(s.fecha_revision, 'YYYY-MM-DD') AS fecha_revision 
                        		, s.tipo_revision
                        		,to_char(s.fecha_auditoria, 'YYYY-MM-DD') AS fecha_auditoria
                        		,fe.identificador identificador_auditor
                        		,fe.nombre ||' '|| fe.apellido nombre_auditor
                        		  
                        	FROM
                        		g_certificacion_bpa.solicitudes s
                        		INNER JOIN g_certificacion_bpa.sitios_areas_productos sap ON s.id_solicitud = sap.id_solicitud
								INNER JOIN g_operadores.sitios st ON s.id_sitio_unidad_produccion = st.id_sitio
                        		INNER JOIN g_operadores.operadores o ON s.identificador_operador = o.identificador
                        		LEFT JOIN 
								f_inspeccion.bpaf01 bp ON bp.id_solicitud = s.id_solicitud and bp.estado_registro = 'Activo'
								INNER JOIN g_uath.ficha_empleado fe ON bp.identificador_usuario = fe.identificador 
                        	WHERE
                        	
                        		s.id_solicitud = $P{idSolicitud}
                        		GROUP BY s.id_solicitud, nombre_operador, st.id_sitio, fe.identificador
                        		ORDER BY s.id_solicitud]]>
	</queryString>
