	<queryString>
		<![CDATA[SELECT
     orden_pago."id_pago" AS orden_pago_id_pago,
     orden_pago."identificador_operador" AS orden_pago_identificador_operador,
     orden_pago."numero_solicitud" AS orden_pago_numero_solicitud,
     orden_pago."fecha_orden_pago" AS orden_pago_fecha_orden_pago,
     orden_pago."total_pagar" AS orden_pago_total_pagar,
     orden_pago."localizacion" AS orden_pago_localizacion,
     clientes."razon_social" AS clientes_razon_social
FROM
     "g_financiero"."orden_pago" orden_pago INNER JOIN "g_financiero"."clientes" clientes ON orden_pago."identificador_operador" = clientes."identificador"
WHERE
     orden_pago."id_pago" = $P{idpago}]]>
