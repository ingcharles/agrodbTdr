	<queryString>
		<![CDATA[SELECT 
	cf.id_certificado_fitosanitario
	, cf.codigo_certificado
	, cf.tipo_certificado
	, cf.nombre_idioma
	, cf.estado_certificado
	, cf.producto_organico
	, cf.nombre_puerto_embarque
	, cf.fecha_embarque
	, COALESCE (STRING_AGG(DISTINCT (COALESCE (ppt.nombre_pais_transito || ' - ', '') || cf.nombre_pais_destino), '; '), '') AS organizacion
	, SUBSTRING (cf.nombre_consignatario || ', ' || cf.direccion_consignatario, 1, 200) AS nombre_direccion_consignatario
	, SUBSTRING (COALESCE (cf.nombre_marca, 'N/A'), 1 , 250) AS marca_distintiva
	, SUBSTRING (COALESCE (cf.informacion_adicional, '') 
		|| CASE WHEN cf.es_reemplazo = 'Si' THEN COALESCE (', El Certificado Fitosanitario Nº ' || cf.codigo_certificado || ', reemplaza y anula al Certificado Fitosanitario Nº ' || cfr.codigo_certificado, '') ELSE '' END 
		|| CASE WHEN cf.producto_organico = 'Si' THEN COALESCE (', Código POA: ' || '(' || string_agg(DISTINCT(ep1.certificacion_organica), ', ') || ')', '') ELSE '' END, 1, 1115) AS informacion_adicional
	, COUNT(DISTINCT(ep1.id_exportador_producto)) AS numero_exportadores
	, cf.nombre_pais_destino
	, cf.es_reemplazo
	, cf.id_certificado_reemplazo
	, SUBSTRING (cf.nombre_pais_origen || ', '	|| cf.nombre_provincia_origen, 1, 64) AS lugar_origen
	, SUBSTRING (cf.nombre_medio_transporte, 1, 64) AS medio_transporte_declarado
	, SUBSTRING (COALESCE (STRING_AGG(DISTINCT (ppt.nombre_pais_transito || ' - ' || ppt.nombre_puerto_transito), ', ') || '; ', '') || COALESCE (STRING_AGG(DISTINCT (cf.nombre_pais_destino || ' - ' || pd.nombre_puerto_pais_destino), ', '), ''), 1, 412) AS punto_entrada_declarado
	, TO_CHAR (cf.fecha_aprobacion_certificado, 'YYYY-MM-DD') AS fecha_aprobacion_certificado
	, ep.id_exportador_producto
	, ep.identificador_exportador
	, SUBSTRING (ep.razon_social_exportador || ', ' || ep.direccion_exportador, 1, 200) || CASE WHEN COUNT(DISTINCT(ep1.id_exportador_producto)) > 1 THEN CASE WHEN cf.nombre_idioma = 'Español' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END AS nombre_direccion_exportador
	, ep.nombre_tipo_producto
	, ep.nombre_subtipo_producto
	, ep.nombre_producto
	, ep.partida_arancelaria_producto
	, SUBSTRING (ep.cantidad_comercial || ' ' || ep.nombre_unidad_cantidad_comercial, 1, 57) || CASE WHEN COUNT(DISTINCT(ep1.id_exportador_producto)) > 1 THEN CASE WHEN cf.nombre_idioma = 'Español' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END AS numero_descripcion_bultos
	, ep.peso_bruto
	, ep.nombre_unidad_peso_bruto
	, ep.peso_neto
	, ep.nombre_unidad_peso_neto
	, ep.nombre_area
	, ep.codigo_centro_acopio
	, ep.nombre_provincia_area
	, COALESCE (SUBSTRING (ep.nombre_tipo_tratamiento || ', ' || ep.nombre_tratamiento, 1, 80), 'N/A') AS tratamiento
	, COALESCE (SUBSTRING (ep.duracion_tratamiento || ' ' || ep.nombre_unidad_duracion, 1, 50), 'N/A') AS duracion_temperatura
	, COALESCE (SUBSTRING (ep.concentracion_tratamiento || ' ' || ep.nombre_unidad_concentracion, 1, 50), 'N/A') AS concentracion
	, ep.temperatura_tratamiento
	, ep.nombre_unidad_temperatura
	, COALESCE (TO_CHAR (ep.fecha_tratamiento, 'YYYY-MM-DD'), 'N/A') AS fecha_tratamiento
	, COALESCE (SUBSTRING (ep.producto_quimico, 1, 80), 'N/A') AS producto_quimico
	, ep.estado_exportador_producto
	, SUBSTRING (CASE WHEN cf.tipo_certificado = 'musaceas' THEN 
		ep.nombre_subtipo_producto || ' - ' || p.nombre_comun || 
	  	E'\n' || 'Peso Neto: ' || ep.peso_neto || ' ' || ep.nombre_unidad_peso_neto || 
	  	E'\n' || 'Peso Bruto: ' || ep.peso_bruto || ' ' || ep.nombre_unidad_peso_bruto || CASE WHEN COUNT(DISTINCT(ep1.id_exportador_producto)) > 1 THEN CASE WHEN cf.nombre_idioma = 'Español' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END
	  ELSE
	  	ep.nombre_subtipo_producto || ' - ' || p.nombre_comun ||
		E'\n' || 'Peso Neto: ' || ep.peso_neto || ' ' || ep.nombre_unidad_peso_neto || CASE WHEN COUNT(DISTINCT(ep1.id_exportador_producto)) > 1 THEN CASE WHEN cf.nombre_idioma = 'Español' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END
	  END, 1, 167) AS producto_cantidad_declarada
	, p.nombre_cientifico || CASE WHEN COUNT(DISTINCT(ep1.id_exportador_producto)) > 1 THEN CASE WHEN cf.nombre_idioma = 'Español' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END AS nombre_botanico
	, 'Por la presente se certifica que las plantas, productos vegetales u otros artículos reglamentados descritos aquí se han inspeccionado y/o sometido a ensayos de acuerdo con los procedimientos oficiales adecuados y se considera que están libres de las plagas cuarentenarias especificadas por la parte contratante importadora y que cumplen los requisitos fitosanitarios vigentes de la parte contratante importadora, incluidos los relativos a las plagas no cuarentenarias reglamentadas.' || E'\nThis is to certify that the plants, plant products or other regulated articles described herein have been inspected and/or tested according to appropiate official procedures and are considered to be free from the quarantine pests specified by the importing contracting party and to conform with the current phytosanitary requirements of the importing contracting party, including those for regulated non-quarantine pests'::text AS texto_nombre_botanico
	, LTRIM(TRIM(STRING_AGG (DISTINCT (r.nombre), ', ')), ' ,') AS nombre_requisito
	, LTRIM(TRIM(STRING_AGG (DISTINCT (r.detalle), ', ')), ' ,') AS detalle_requisito
	, COALESCE (CASE WHEN LENGTH (TRIM(STRING_AGG (DISTINCT (r.detalle_impreso), ', '), ', ')) <= '400' THEN SUBSTRING (TRIM(STRING_AGG (DISTINCT (r.detalle_impreso), ', '), ', '), 1, 400) || CASE WHEN COUNT(DISTINCT(ep1.id_exportador_producto)) > 1 THEN CASE WHEN cf.nombre_idioma = 'Español' THEN ' / (Ver Anexo)' ELSE ' / (See Annex)' END ELSE '' END END, 'N/A') AS detalle_impreso
	, TRIM(STRING_AGG (DISTINCT (r.codigo), ', ')) AS codigo_requisito
	, CASE WHEN cf.tipo_certificado = 'musaceas' THEN
		CASE WHEN cf.nombre_idioma = 'Español' THEN
			to_char(cf.fecha_embarque, 'TMDay' || ' dd ') || 'de ' || to_char(cf.fecha_embarque, 'TMmonth') || ' de ' || to_char(cf.fecha_embarque, 'yyyy') 
		ELSE
			to_char(cf.fecha_embarque, 'Day' || ' dd ') || TRIM(to_char(cf.fecha_embarque, 'month')) || to_char(cf.fecha_embarque, ' YYYY')
		END
	  ELSE
	  	CASE WHEN cf.nombre_idioma = 'Español' THEN
			to_char(cf.fecha_aprobacion_certificado, 'TMDay' || ' dd ') || 'de ' || to_char(cf.fecha_aprobacion_certificado, 'TMmonth') || ' de ' || to_char(cf.fecha_aprobacion_certificado, 'yyyy') 
		ELSE
			to_char(cf.fecha_aprobacion_certificado, 'Day' || ' dd ') || TRIM(to_char(cf.fecha_aprobacion_certificado, 'month')) || to_char(cf.fecha_aprobacion_certificado, ' YYYY')
		END
	  END AS fecha_aprobacion_certificado_letras
FROM 
	g_certificado_fitosanitario.certificado_fitosanitario cf
	INNER JOIN g_certificado_fitosanitario.exportadores_productos ep ON cf.id_certificado_fitosanitario = ep.id_certificado_fitosanitario
	INNER JOIN g_certificado_fitosanitario.exportadores_productos ep1 ON cf.id_certificado_fitosanitario = ep1.id_certificado_fitosanitario
	INNER JOIN g_certificado_fitosanitario.puertos_destino pd ON cf.id_certificado_fitosanitario = pd.id_certificado_fitosanitario
	INNER JOIN g_catalogos.productos p ON p.id_producto = ep.id_producto
	INNER JOIN g_requisitos.requisitos_comercializacion rc ON rc.id_producto = ep.id_producto
	INNER JOIN g_requisitos.requisitos_asignados ra ON rc.id_requisito_comercio = ra.id_requisito_comercio
	INNER JOIN g_requisitos.requisitos r ON ra.requisito = r.id_requisito
	LEFT JOIN g_certificado_fitosanitario.paises_puertos_transito ppt ON cf.id_certificado_fitosanitario = ppt.id_certificado_fitosanitario
	LEFT JOIN g_certificado_fitosanitario.certificado_fitosanitario cfr ON cf.id_certificado_reemplazo = cfr.id_certificado_fitosanitario	
WHERE
	cf.id_certificado_fitosanitario = $P{idSolicitud} 
	AND rc.id_localizacion = cf.id_pais_destino
	AND ra.tipo = 'Exportación'
	AND r.tipo = 'Exportación'
	AND r.estado = 1
	AND ep.estado_exportador_producto NOT IN ('Rechazado')
GROUP BY cf.id_certificado_fitosanitario, p.nombre_comun, p.nombre_cientifico, ep.id_exportador_producto, cfr.codigo_certificado
ORDER BY ep.id_exportador_producto ASC
LIMIT 1]]>
	</queryString>
