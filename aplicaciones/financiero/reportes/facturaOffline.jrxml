	<parameter name="fondoCertificado" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     ( SELECT sum(cantidad*precio_unitario)::numeric(8,2) FROM "g_financiero"."detalle_pago" WHERE id_pago = $P{idpago} and iva = 0 ) as total_sin_iva,
     ( SELECT sum(cantidad*precio_unitario)::numeric(8,2) FROM "g_financiero"."detalle_pago" WHERE id_pago = $P{idpago} and iva != 0 ) as total_con_iva,
     ( SELECT sum(descuento)::numeric(8,2) FROM "g_financiero"."detalle_pago" WHERE id_pago = $P{idpago} ) as descuento,
     ( SELECT sum(iva)::numeric(8,2) FROM "g_financiero"."detalle_pago" WHERE id_pago = $P{idpago} ) as suma_iva,
     ( SELECT sum(subsidio)::numeric(8,2) FROM g_financiero.detalle_pago WHERE id_pago  = $P{idpago}) as subsidio,
     orden_pago."total_pagar" AS orden_pago_total_pagar,
	 orden_pago."localizacion" AS orden_pago_localizacion,
	 orden_pago."numero_establecimiento" ||'-'|| orden_pago."punto_emision" ||'-'||orden_pago."numero_factura" AS orden_pago_numero_factura,
	 orden_pago."fecha_facturacion" AS orden_pago_fecha_facturacion,
	 orden_pago."numero_autorizacion" AS orden_pago_numero_autorizacion,
	 orden_pago."fecha_autorizacion" AS orden_pago_fecha_autorizacion,
	 orden_pago."clave_acceso" AS orden_pago_clave_acceso,
	 orden_pago."tipo_emision" AS orden_pago_tipo_emision,
	 clientes."razon_social" AS clientes_razon_social,
	 clientes."direccion" AS clientes_direccion,
	 clientes."telefono" AS clientes_telefono,
	 clientes."correo" AS clientes_correo,
	 clientes."identificador" AS clientes_identificador,
	 distritos."ruc" AS distritos_ruc,
	 distritos."razon_social" AS distritos_razon_social,
	 distritos."direccion" AS distritos_direccion,
	 distritos."obligado_llevar_contabilidad" AS distritos_obligado_llevar_contabilidad,
	 distritos."nombre_comercial" AS distritos_nombre_comercial
FROM
     "g_financiero"."clientes" clientes INNER JOIN "g_financiero"."orden_pago" orden_pago ON clientes."identificador" = orden_pago."identificador_operador"
     INNER JOIN "g_financiero"."distritos" distritos ON orden_pago."ruc_institucion" = distritos."ruc"
     INNER JOIN "g_financiero"."oficina_recaudacion" oficina_recaudacion ON distritos."ruc" = oficina_recaudacion."ruc"
WHERE
     orden_pago."id_pago" = $P{idpago} and oficina_recaudacion."identificador_firmante" = (SELECT orden_pago."identificador_firmante" FROM "g_financiero"."orden_pago" orden_pago WHERE orden_pago."id_pago" = $P{idpago})]]>
	</queryString>
