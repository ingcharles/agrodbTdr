	<subDataset name="New Dataset 1" uuid="22d3af51-1868-402e-ae34-0a1380afa293">
		<parameter name="idPagoDetallo" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
     detalle_pago."concepto_orden" AS detalle_pago_concepto_orden,
     detalle_pago."cantidad" AS detalle_pago_cantidad,
     (detalle_pago."precio_unitario")::double precision AS detalle_pago_precio_unitario,
     detalle_pago."descuento" AS detalle_pago_descuento,
     detalle_pago."iva" AS detalle_pago_iva,
     detalle_pago."total" AS detalle_pago_total,
     detalle_pago."id_pago" AS detalle_pago_id_pago
FROM
     "g_financiero"."detalle_pago" detalle_pago
WHERE
     id_pago = $P{idPagoDetallo}]]>
		</queryString>
		<field name="detalle_pago_concepto_orden" class="java.lang.String"/>
		<field name="detalle_pago_cantidad" class="java.lang.Double"/>
