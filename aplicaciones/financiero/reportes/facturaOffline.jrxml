	<subDataset name="New Dataset 1" uuid="ef677ce2-d5e5-48de-b27c-92231fea0db8">
		<parameter name="idPagoDetalle" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
     detalle_pago."precio_unitario"::double precision AS detalle_pago_precio_unitario,
     detalle_pago."concepto_orden" AS detalle_pago_concepto_orden,
     detalle_pago."cantidad" AS detalle_pago_cantidad,
     detalle_pago."descuento" AS detalle_pago_descuento,
     detalle_pago."iva" AS detalle_pago_iva,
     detalle_pago."total" AS detalle_pago_total,
     servicios."codigo" AS servicios_codigo,
     detalle_pago."subsidio" AS detalle_pago_subsidio
FROM
     "g_financiero"."servicios" servicios INNER JOIN "g_financiero"."detalle_pago" detalle_pago ON servicios."id_servicio" = detalle_pago."id_servicio"
WHERE
     id_pago = $P{idPagoDetalle}]]>
		</queryString>
		<field name="detalle_pago_precio_unitario" class="java.lang.Double"/>
		<field name="detalle_pago_concepto_orden" class="java.lang.String"/>
		<field name="detalle_pago_cantidad" class="java.lang.Double"/>
		<field name="detalle_pago_descuento" class="java.math.BigDecimal"/>
		<field name="detalle_pago_iva" class="java.math.BigDecimal"/>
		<field name="detalle_pago_total" class="java.math.BigDecimal"/>
		<field name="servicios_codigo" class="java.lang.String"/>
		<field name="detalle_pago_subsidio" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="idpago" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="datosDeposito" class="java.lang.String"/>
	<parameter name="solicitudAtendida" class="java.lang.String"/>
	<parameter name="observacion" class="java.lang.String"/>
	<parameter name="compensacion" class="java.lang.String"/>
