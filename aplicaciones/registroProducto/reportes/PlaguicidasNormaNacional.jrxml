	<subDataset name="DatasetFabricanteFormuladorManufacturador" uuid="72b0d399-c5d2-46e6-b580-66ab8c164dea">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="conexionLocal"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="idProducto" class="java.lang.Integer"/>
		<queryString>
			<![CDATA[SELECT
	ff.tipo,
	ff.nombre,
	ff.pais_origen
FROM
	g_catalogos.fabricante_formulador ff
WHERE
	ff.estado = 'activo' and
	ff.id_producto =   $P{idProducto} 
UNION
SELECT
	'Manufacturador' as tipo,
	m.manufacturador as nombre,
	m.pais_origen
FROM
	g_catalogos.manufacturador m 
	INNER JOIN g_catalogos.fabricante_formulador ff ON m.id_fabricante_formulador = ff.id_fabricante_formulador
WHERE
	m.estado = 'activo' and
	ff.id_producto = $P{idProducto}]]>
		</queryString>
		<field name="tipo" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="tipo"/>
		</field>
		<field name="nombre" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="nombre"/>
		</field>
		<field name="pais_origen" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="pais_origen"/>
		</field>
	</subDataset>
	<parameter name="idProducto" class="java.lang.Integer"/>
	<parameter name="rutaFondo" class="java.lang.String"/>
	<parameter name="ruta" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	tp.id_area,
	tp.nombre as tipo_producto,
	sp.nombre as subtipo_producto,
	p.nombre_comun as nombre_producto,
	p.nombre_cientifico,
	pi.formulacion,
	pi.numero_registro,
	pi.periodo_reingreso,
	pi.categoria_toxicologica,
	pi.fecha_registro,
	pi.id_operador as ruc,					
	pi.declaracion_venta,
	pi.estabilidad,
	p.ruta,
	CASE p.estado
		WHEN 1 THEN 'Vigente'
		WHEN 2 THEN 'Suspendido'
		WHEN 3 THEN 'Caducado'
		WHEN 4 THEN 'Cancelado'
		ELSE 'Eliminado'
	END as estado,
	CASE 
	WHEN count(o.identificador) >= 1
	THEN
		o.razon_social
	ELSE
		epp.razon_social
	END as razon_social
FROM
	g_catalogos.productos p
	INNER JOIN g_catalogos.subtipo_productos sp ON p.id_subtipo_producto = sp.id_subtipo_producto
	INNER JOIN g_catalogos.tipo_productos tp ON sp.id_tipo_producto = tp.id_tipo_producto
	INNER JOIN g_catalogos.productos_inocuidad pi ON p.id_producto = pi.id_producto
	LEFT JOIN g_operadores.operadores o ON pi.id_operador = o.identificador
	LEFT JOIN g_catalogos.empresa_producto_plaguicida epp ON pi.id_operador = epp.identificador
WHERE
	p.id_producto =   $P{idProducto}
GROUP BY tp.id_area,
	tp.nombre,
	sp.nombre,
	p.nombre_comun,
	p.nombre_cientifico,
	pi.formulacion,
	pi.numero_registro,
	pi.periodo_reingreso,
	pi.categoria_toxicologica,
	pi.fecha_registro,
	pi.id_operador,	
	pi.declaracion_venta,
	pi.estabilidad,
	p.ruta,
	p.estado,
	o.razon_social,
	epp.razon_social]]>
	</queryString>
