	<parameter name="idProducto" class="java.lang.Integer"/>
	<parameter name="rutaFondo" class="java.lang.String"/>
	<parameter name="ruta" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
	tp.id_area,
	tp.nombre as tipo_producto,
	sp.nombre as subtipo_producto,
	p.nombre_comun as nombre_producto,
	p.nombre_cientifico,
	pi.formulacion,
	pi.numero_registro,
	pi.periodo_reingreso,
	pi.categoria_toxicologica,
	pi.fecha_registro,
	pi.id_operador as ruc,					
	pi.declaracion_venta,
	pi.estabilidad,
	p.ruta,
	CASE p.estado
		WHEN 1 THEN 'Vigente'
		WHEN 2 THEN 'Suspendido'
		WHEN 3 THEN 'Caducado'
		WHEN 4 THEN 'Cancelado'
		ELSE 'Eliminado'
	END as estado,
	CASE 
	WHEN count(o.identificador) >= 1
	THEN
		o.razon_social
	ELSE
		epp.razon_social
	END as razon_social
FROM
	g_catalogos.productos p
	INNER JOIN g_catalogos.subtipo_productos sp ON p.id_subtipo_producto = sp.id_subtipo_producto
	INNER JOIN g_catalogos.tipo_productos tp ON sp.id_tipo_producto = tp.id_tipo_producto
	INNER JOIN g_catalogos.productos_inocuidad pi ON p.id_producto = pi.id_producto
